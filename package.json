{
  "name": "@byyuurin/eslint-config",
  "type": "module",
  "version": "1.3.1",
  "packageManager": "pnpm@9.5.0",
  "license": "MIT",
  "homepage": "https://github.com/byyuurin/eslint-config",
  "repository": {
    "type": "git",
    "url": "https://github.com/byyuurin/eslint-config.git"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "nr type-gen && tsup --format esm,cjs --clean --dts",
    "stub": "tsup --format esm",
    "dev": "tsup --format esm,cjs --watch & eslint-config-inspector",
    "watch": "tsup --format esm,cjs --watch",
    "lint": "pnpm run stub && eslint .",
    "lint:fix": "pnpm run stub && eslint . --fix",
    "release": "bumpp && pnpm publish",
    "test": "vitest",
    "type-gen": "esno scripts/type-gen.ts",
    "type-check": "tsc --noEmit",
    "prepack": "nr build",
    "prepare": "simple-git-hooks"
  },
  "peerDependencies": {
    "@unocss/eslint-plugin": ">=0.50.0",
    "eslint": ">=8.50.0",
    "eslint-plugin-format": ">=0.1.0"
  },
  "peerDependenciesMeta": {
    "@unocss/eslint-plugin": {
      "optional": true
    },
    "eslint-plugin-format": {
      "optional": true
    }
  },
  "dependencies": {
    "@antfu/install-pkg": "^0.3.3",
    "@eslint/js": "^9.7.0",
    "@stylistic/eslint-plugin": "^2.3.0",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1",
    "eslint-config-flat-gitignore": "^0.1.8",
    "eslint-flat-config-utils": "^0.2.5",
    "eslint-merge-processors": "^0.1.0",
    "eslint-plugin-antfu": "^2.3.4",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import-x": "^3.0.1",
    "eslint-plugin-jsdoc": "^48.8.1",
    "eslint-plugin-jsonc": "^2.16.0",
    "eslint-plugin-markdown": "^5.1.0",
    "eslint-plugin-n": "^17.9.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-toml": "^0.11.1",
    "eslint-plugin-unicorn": "^54.0.0",
    "eslint-plugin-unused-imports": "^3.2.0",
    "eslint-plugin-vue": "^9.27.0",
    "eslint-plugin-yml": "^1.14.0",
    "eslint-processor-vue-blocks": "^0.1.2",
    "globals": "^15.8.0",
    "jsonc-eslint-parser": "^2.4.0",
    "local-pkg": "^0.5.0",
    "prompts": "^2.4.2",
    "toml-eslint-parser": "^0.10.0",
    "vue-eslint-parser": "^9.4.3",
    "yaml-eslint-parser": "^1.2.3"
  },
  "devDependencies": {
    "@antfu/ni": "^0.22.0",
    "@byyuurin/eslint-config": "workspace:*",
    "@eslint/config-inspector": "^0.5.2",
    "@stylistic/eslint-plugin-migrate": "^2.3.0",
    "@types/eslint": "^8.56.10",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.14.11",
    "@types/prompts": "^2.4.9",
    "@unocss/eslint-plugin": "^0.61.5",
    "bumpp": "^9.4.1",
    "eslint": "^9.7.0",
    "eslint-plugin-format": "^0.1.2",
    "eslint-typegen": "^0.2.4",
    "esno": "^4.7.0",
    "execa": "^9.3.0",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "lint-staged": "^15.2.7",
    "simple-git-hooks": "^2.11.1",
    "tsup": "^8.2.0",
    "typescript": "^5.5.3",
    "vitest": "^2.0.3",
    "vue": "^3.4.33"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
