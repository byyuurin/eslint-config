{
  "name": "@byyuurin/eslint-config",
  "type": "module",
  "version": "1.0.0-beta.15",
  "packageManager": "pnpm@8.15.1",
  "license": "MIT",
  "homepage": "https://github.com/byyuurin/eslint-config",
  "repository": {
    "type": "git",
    "url": "https://github.com/byyuurin/eslint-config.git"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup --format esm,cjs --clean --dts",
    "stub": "tsup --format esm",
    "dev": "tsup --format esm,cjs --watch & eslint-flat-config-viewer",
    "watch": "tsup --format esm,cjs --watch",
    "lint": "pnpm run stub && eslint .",
    "lint:fix": "pnpm run stub && eslint . --fix",
    "prepack": "nr build",
    "release": "bumpp && pnpm publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "peerDependencies": {
    "eslint": ">=8.50.0",
    "eslint-plugin-format": ">=0.1.0"
  },
  "peerDependenciesMeta": {
    "@unocss/eslint-plugin": {
      "optional": true
    },
    "eslint-plugin-format": {
      "optional": true
    }
  },
  "dependencies": {
    "@antfu/eslint-define-config": "1.23.0-2",
    "@antfu/install-pkg": "^0.3.1",
    "@eslint-types/jsdoc": "^48.0.4",
    "@eslint-types/typescript-eslint": "^6.19.1",
    "@eslint-types/unicorn": "^50.0.1",
    "@eslint/js": "^8.56.0",
    "@stylistic/eslint-plugin": "^1.5.4",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "eslint-config-flat-gitignore": "^0.1.2",
    "eslint-merge-processors": "^0.1.0",
    "eslint-plugin-antfu": "^2.1.2",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-i": "^2.29.1",
    "eslint-plugin-jsdoc": "^48.0.4",
    "eslint-plugin-jsonc": "^2.13.0",
    "eslint-plugin-markdown": "^3.0.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-toml": "^0.9.2",
    "eslint-plugin-unicorn": "^50.0.1",
    "eslint-plugin-unused-imports": "^3.0.0",
    "eslint-plugin-vue": "^9.21.1",
    "eslint-plugin-yml": "^1.12.2",
    "eslint-processor-vue-blocks": "^0.1.1",
    "globals": "^13.24.0",
    "jsonc-eslint-parser": "^2.4.0",
    "local-pkg": "^0.5.0",
    "prompts": "^2.4.2",
    "toml-eslint-parser": "^0.9.3",
    "vue-eslint-parser": "^9.4.2",
    "yaml-eslint-parser": "^1.2.2"
  },
  "devDependencies": {
    "@antfu/ni": "^0.21.12",
    "@byyuurin/eslint-config": "workspace:*",
    "@stylistic/eslint-plugin-migrate": "^1.5.4",
    "@types/eslint": "^8.56.2",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.11.16",
    "@types/prompts": "^2.4.9",
    "@unocss/eslint-plugin": "^0.58.4",
    "bumpp": "^9.3.0",
    "eslint": "^8.56.0",
    "eslint-flat-config-viewer": "^0.1.11",
    "eslint-plugin-format": "^0.1.0",
    "execa": "^8.0.1",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "lint-staged": "^15.2.1",
    "simple-git-hooks": "^2.9.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.2",
    "vue": "^3.4.15"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
