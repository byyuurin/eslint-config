{
  "private": true,
  "license": "MIT",
  "packageManager": "pnpm@8.10.2",
  "version": "0.15.1",
  "name": "@byyuurin/eslint-config-monorepo",
  "repository": {
    "type": "git",
    "url": "https://github.com/byyuurin/eslint-config.git"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "release": "bumpp -r && pnpm -r publish"
  },
  "devDependencies": {
    "@byyuurin/eslint-config": "workspace:*",
    "@stylistic/eslint-plugin-migrate": "^1.0.0",
    "@types/node": "^18.18.8",
    "bumpp": "^9.2.0",
    "eslint": "^8.53.0",
    "typescript": "^5.2.2",
    "vue": "^3.3.7"
  },
  "eslintConfig": {
    "plugins": [
      "@stylistic/migrate"
    ],
    "extends": "@byyuurin",
    "rules": {
      "@stylistic/migrate/migrate-js": "error",
      "@stylistic/migrate/migrate-ts": "error"
    }
  }
}
