{
  "name": "@byyuurin/eslint-config",
  "type": "module",
  "version": "1.1.0-beta.2",
  "packageManager": "pnpm@8.15.5",
  "license": "MIT",
  "homepage": "https://github.com/byyuurin/eslint-config",
  "repository": {
    "type": "git",
    "url": "https://github.com/byyuurin/eslint-config.git"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "nr type-gen && tsup --format esm,cjs --clean --dts",
    "stub": "tsup --format esm",
    "dev": "tsup --format esm,cjs --watch & eslint-config-inspector",
    "watch": "tsup --format esm,cjs --watch",
    "lint": "pnpm run stub && eslint .",
    "lint:fix": "pnpm run stub && eslint . --fix",
    "release": "bumpp && pnpm publish",
    "test": "vitest",
    "type-gen": "esno scripts/rules-type-gen.ts",
    "type-check": "tsc --noEmit",
    "prepack": "nr build",
    "prepare": "simple-git-hooks"
  },
  "peerDependencies": {
    "eslint": ">=8.50.0",
    "eslint-plugin-format": ">=0.1.0"
  },
  "peerDependenciesMeta": {
    "@unocss/eslint-plugin": {
      "optional": true
    },
    "eslint-plugin-format": {
      "optional": true
    }
  },
  "dependencies": {
    "@antfu/install-pkg": "^0.3.2",
    "@eslint/js": "^8.57.0",
    "@stylistic/eslint-plugin": "^1.7.0",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "eslint-config-flat-gitignore": "^0.1.3",
    "eslint-flat-config-utils": "^0.1.2",
    "eslint-merge-processors": "^0.1.0",
    "eslint-plugin-antfu": "^2.1.2",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import-x": "^0.4.4",
    "eslint-plugin-jsdoc": "^48.2.2",
    "eslint-plugin-jsonc": "^2.14.1",
    "eslint-plugin-markdown": "^4.0.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "eslint-plugin-toml": "^0.10.0",
    "eslint-plugin-unicorn": "^51.0.1",
    "eslint-plugin-unused-imports": "^3.1.0",
    "eslint-plugin-vue": "^9.24.0",
    "eslint-plugin-yml": "^1.13.2",
    "eslint-processor-vue-blocks": "^0.1.1",
    "globals": "^15.0.0",
    "jsonc-eslint-parser": "^2.4.0",
    "local-pkg": "^0.5.0",
    "prompts": "^2.4.2",
    "toml-eslint-parser": "^0.9.3",
    "vue-eslint-parser": "^9.4.2",
    "yaml-eslint-parser": "^1.2.2"
  },
  "devDependencies": {
    "@antfu/ni": "^0.21.12",
    "@byyuurin/eslint-config": "workspace:*",
    "@eslint/config-inspector": "^0.1.0",
    "@stylistic/eslint-plugin-migrate": "^1.7.0",
    "@types/eslint": "^8.56.6",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.12.2",
    "@types/prompts": "^2.4.9",
    "@unocss/eslint-plugin": "^0.58.8",
    "bumpp": "^9.4.0",
    "eslint": "^8.57.0",
    "eslint-plugin-format": "^0.1.0",
    "eslint-typegen": "^0.2.0",
    "esno": "^4.7.0",
    "execa": "^8.0.1",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "lint-staged": "^15.2.2",
    "simple-git-hooks": "^2.11.1",
    "tsup": "^8.0.2",
    "typescript": "^5.4.3",
    "vitest": "^1.4.0",
    "vue": "^3.4.21"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
