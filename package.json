{
  "name": "@byyuurin/eslint-config",
  "type": "module",
  "version": "1.7.0-beta.1",
  "packageManager": "pnpm@9.15.3",
  "license": "MIT",
  "homepage": "https://github.com/byyuurin/eslint-config",
  "repository": {
    "type": "git",
    "url": "https://github.com/byyuurin/eslint-config.git"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "nr type-gen && tsup --format esm,cjs --clean --dts",
    "stub": "tsup --format esm",
    "dev": "tsup --format esm,cjs --watch & eslint-config-inspector",
    "watch": "tsup --format esm,cjs --watch",
    "lint": "pnpm run stub && eslint .",
    "lint:fix": "pnpm run stub && eslint . --fix",
    "release": "bumpp && pnpm publish",
    "test": "vitest",
    "type-gen": "esno scripts/type-gen.ts",
    "type-check": "tsc --noEmit",
    "prepack": "nr build",
    "prepare": "simple-git-hooks"
  },
  "peerDependencies": {
    "@unocss/eslint-plugin": ">=0.50.0",
    "eslint": ">=8.57.1 || >=9.10.0",
    "eslint-plugin-format": ">=0.1.0"
  },
  "peerDependenciesMeta": {
    "@unocss/eslint-plugin": {
      "optional": true
    },
    "eslint-plugin-format": {
      "optional": true
    }
  },
  "dependencies": {
    "@antfu/install-pkg": "^1.0.0",
    "@eslint/js": "^9.18.0",
    "@stylistic/eslint-plugin": "^2.12.1",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "eslint-config-flat-gitignore": "^1.0.0",
    "eslint-flat-config-utils": "^1.0.0",
    "eslint-merge-processors": "^1.0.0",
    "eslint-plugin-antfu": "^2.7.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import-x": "^4.6.1",
    "eslint-plugin-jsdoc": "^50.6.1",
    "eslint-plugin-jsonc": "^2.18.2",
    "eslint-plugin-markdown": "^5.1.0",
    "eslint-plugin-n": "^17.15.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-toml": "^0.12.0",
    "eslint-plugin-unicorn": "^56.0.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "eslint-plugin-vue": "^9.32.0",
    "eslint-plugin-yml": "^1.16.0",
    "eslint-processor-vue-blocks": "^1.0.0",
    "globals": "^15.14.0",
    "jsonc-eslint-parser": "^2.4.0",
    "local-pkg": "^1.0.0",
    "prompts": "^2.4.2",
    "toml-eslint-parser": "^0.10.0",
    "vue-eslint-parser": "^9.4.3",
    "yaml-eslint-parser": "^1.2.3"
  },
  "devDependencies": {
    "@antfu/ni": "^23.2.0",
    "@byyuurin/eslint-config": "workspace:*",
    "@eslint/config-inspector": "^0.7.1",
    "@stylistic/eslint-plugin-migrate": "^2.12.1",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.10.5",
    "@types/prompts": "^2.4.9",
    "@unocss/eslint-plugin": "^65.4.0",
    "bumpp": "^9.10.0",
    "eslint": "^9.18.0",
    "eslint-plugin-format": "^1.0.1",
    "eslint-typegen": "^1.0.0",
    "esno": "^4.8.0",
    "execa": "^9.5.2",
    "fast-glob": "^3.3.3",
    "fs-extra": "^11.2.0",
    "lint-staged": "^15.3.0",
    "simple-git-hooks": "^2.11.1",
    "tsup": "^8.3.5",
    "typescript": "^5.7.3",
    "vitest": "^2.1.8",
    "vue": "^3.5.13"
  },
  "pnpm": {
    "overrides": {
      "is-core-module": "npm:@nolyfill/is-core-module@^1.0.39"
    }
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
